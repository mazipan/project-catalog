{
    "name": "project-catalog",
    "version": "0.0.4",
    "description": "Irfan Maulana Projects Catalog",
    "main": "./index.html",
    "homepage": "https://mazipan.github.io/project-catalog",
    "author": "Irfan Maulana <mazipanneh@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
        "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --color --open --inline",
        "lint": "eslint -- .",
        "format": "prettier-standard 'src/**/*.js'",
        "publish": "node ./gh-publish.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mazipan/project-catalog.git"
    },
    "bugs": {
        "url": "https://github.com/mazipan/project-catalog/issues"
    },
    "engines": {
        "node": ">= 4.0.0",
        "npm": ">= 3.0.0"
    },
    "keywords": [
        "react alert",
        "react sweatalert",
        "react simple alert",
        "react simplert"
    ],
    "eslintConfig": {
        "parser": "babel-eslint",
        "extends": [
            "standard",
            "standard-jsx"
        ],
        "env": {
            "node": true,
            "es6": true
        },
        "plugins": [
            "react"
        ],
        "parserOptions": {
            "ecmaVersion": 6,
            "sourceType": "module",
            "ecmaFeatures": {
                "jsx": true
            }
        },
        "globals": {},
        "rules": {}
    },
    "eslintIgnore": [
        "webpack.config.js",
        "webpack.dist.config.js",
        "build/"
    ],
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-eslint": "8.0.2",
        "babel-loader": "^7.1.2",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "css-loader": "^0.28.7",
        "compression-webpack-plugin": "^1.0.1",
        "cross-env": "^5.1.1",
        "es6-promise": "^4.1.1",
        "eslint": "4.11.0",
        "eslint-config-standard": "10.2.1",
        "eslint-config-standard-jsx": "4.0.2",
        "eslint-plugin-import": "2.8.0",
        "eslint-plugin-node": "5.2.1",
        "eslint-plugin-promise": "3.6.0",
        "eslint-plugin-react": "7.4.0",
        "eslint-plugin-standard": "3.0.1",
        "gh-pages": "1.0.0",
        "html-webpack-plugin": "2.30.1",
        "node-sass": "^4.6.1",
        "prettier-standard": "7.0.3",
        "react-dom": "^16.1.1",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.19.0",
        "webpack": "^3.8.1",
        "webpack-dev-server": "^2.9.4"
    },
    "dependencies": {
        "prop-types": "^15.6.0",
        "react": "^16.1.1"
    }
}